    <script>
        function validateEmail() {
            var reg = /^([A-Za-z0-9_\-\.])+\@@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
            if (reg.test($("#email").val()) == false) {
                alert('Please enter valid email id');
                return false;
            }
            else {
                return true;
            }
        }
    </script>
