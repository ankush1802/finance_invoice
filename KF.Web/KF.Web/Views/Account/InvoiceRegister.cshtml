    @{
        if (TempData["res"] != null)
        {
            if ((int)TempData["res"] > 0)
            {
                using (var db = new KF.Entity.KFentities())
                {
                    var Id = (int)TempData["res"];
                    var data = db.UserRegistrations.Where(x => x.Id == Id).FirstOrDefault();
                    var state = db.Provinces.Where(x => x.Id == data.ProvinceId).FirstOrDefault();

                    <script>
                        $("#reslogin").css("display", "none");
                        $("#shippingaddress").text("@data.CorporationAddress" + '\r\n' + "@state.ProvinceName" + '-' + "@data.City" + '\r\n' + "@data.PostalCode");
                        $("#corporationaddress").text("@data.CorporationAddress" + '\r\n' + "@state.ProvinceName" + '-' + "@data.City" + '\r\n' + "@data.PostalCode");

                        $("#corporatestreet").val("@data.CorporationAddress");
                        $("#corporatecity").val("@data.City");
                        $("#corporatestate").val("@state.ProvinceName");
                        $("#corporatepostalcode").val("@data.PostalCode");

                        $("#shippingstreet").val("@data.CorporationAddress");
                        $("#shippingcity").val("@data.City");
                        $("#shippingstate").val("@state.ProvinceName");
                        $("#shippingpostalcode").val("@data.PostalCode");

                        $("#reslogin").css("display", "none");

                    </script>

                }
            }

        }
