    <script>
        function myFunctionLive() {
            if ($("#username").val() != "" && $("#tradingcurrency").val() != "" && ($("#companyname").val() != "")
                && ($("#corporatestreet").val() != "") && ($("#corporatecity").val() != "")
                && ($("#corporatestate").val() != "") && ($("#corporatepostalcode").val() != "")
                && ($("#businessNumber").val() != "")
                && ($("#shippingstreet").val() != "") && ($("#shippingcity").val() != "")
                && ($("#shippingstate").val() != "") && ($("#shippingpostalcode").val() != "")
                ) {

                function isEmailValid(emailid) {
                    var pattern = new RegExp(/\w+([-+.']\w+)*@@\w+([-.]\w+)*\.\w+([-.]\w+)*/);
                    return pattern.test(emailid);
                };
                if ($("#email").val().length == 0 || !isEmailValid($("#email").val())) {

                    alert("Please fill mandatory fields.");
                }
                else {
                    if ($("#chkTermsConditions").is(':checked') == true) {

                        $("#hdcorptno").val($("#corporateaptno").val());
                        $("#hdcorhno").val($("#corporatehouseno").val());
                        $("#hdcorstr").val($("#corporatestreet").val());
                        $("#hdcorci").val($("#corporatecity").val());
                        $("#hdcorst").val($("#corporatestate").val());
                        $("#hdcorpcode").val($("#corporatepostalcode").val());

                        $("#hdshiptno").val($("#shippingaptno").val());
                        $("#hdshiphno").val($("#shippinghouseno").val());
                        $("#hdshipstr").val($("#shippingstreet").val());
                        $("#hdshipci").val($("#shippingcity").val());
                        $("#hdshipst").val($("#shippingstate").val());
                        $("#hdshippcode").val($("#shippingpostalcode").val());

                        $('form').submit();

                        return true;
                    }
                    else {
                        alert("Please read and accept Terms & Conditions.");
                    }




                }

            }
            else {

                alert("Please fill mandatory fields.");

            }


        }
    </script>
    <script>

        $(document).ready(function () {
            //called when key is pressed in textbox
            $(".numberOnly").keypress(function (e) {
                //if the letter is not digit then display error and don't type anything
                if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                    //display error message
                    //  $("#errmsg").html("Digits Only").show().fadeOut("slow");
                    return false;
                }
            });

            $("#btnAddCorporate").click(function () {
                if ($("#corporatepostalcode").val().length <= 5) {
                    alert("Postal Code should be of 6 characters.");
                    $('#dialog').dialog('open');
                    return false;
                }
                else {


                    var corporateaptno = $("#corporateaptno").val();
                    var corporatehouseno = $("#corporatehouseno").val();
                    var corporatestreet = $("#corporatestreet").val();
                    var corporatecity = $("#corporatecity").val();
                    var corporatestate = $("#corporatestate").val();
                    var corporatepostalcode = $("#corporatepostalcode").val();

                    $("#hdcorptno").val(corporateaptno);
                    $("#hdcorhno").val(corporatehouseno);
                    $("#hdcorstr").val(corporatestreet);
                    $("#hdcorci").val(corporatecity);
                    $("#hdcorst").val(corporatestate);
                    $("#hdcorpcode").val(corporatepostalcode);

                    $("#hdshiptno").val(corporateaptno);
                    $("#hdshiphno").val(corporatehouseno);
                    $("#hdshipstr").val(corporatestreet);
                    $("#hdshipci").val(corporatecity);
                    $("#hdshipst").val(corporatestate);
                    $("#hdshippcode").val(corporatepostalcode);

                    if (($("#corporatestreet").val() != "") && ($("#corporatecity").val() != "")
                        && ($("#corporatestate").val() != "") && ($("#corporatepostalcode").val() != "")) {


                        if (corporateaptno != "" && corporatehouseno != "") {
                            $("#corporationaddress").text(corporateaptno + '-' + corporatehouseno + '\r\n' + corporatestreet + '\r\n' + corporatecity + ' ' + corporatestate + '\r\n' + corporatepostalcode);
                        }
                        else {


                            if (corporateaptno == "" && corporatehouseno == "") {
                                $("#corporationaddress").text(corporatestreet + '\r\n' + corporatecity + ' ' + corporatestate + '\r\n' + corporatepostalcode);
                            }
                            else {
                                $("#corporationaddress").text(corporateaptno + ' ' + corporatehouseno + '\r\n' + corporatestreet + '\r\n' + corporatecity + ' ' + corporatestate + '\r\n' + corporatepostalcode);
                            }

                        }

                        if ($("#shippingaddress").text() == "") {



                            if (corporateaptno != "" && corporatehouseno != "") {
                                $("#shippingaddress").text(corporateaptno + '-' + corporatehouseno + '\r\n' + corporatestreet + '\r\n' + corporatecity + ' ' + corporatestate + '\r\n' + corporatepostalcode);
                            }
                            else {

                                if (corporateaptno == "" && corporatehouseno == "") {
                                    $("#shippingaddress").text(corporatestreet + '\r\n' + corporatecity + ' ' + corporatestate + '\r\n' + corporatepostalcode);
                                }
                                else {
                                    $("#shippingaddress").text(corporateaptno + ' ' + corporatehouseno + '\r\n' + corporatestreet + '\r\n' + corporatecity + ' ' + corporatestate + '\r\n' + corporatepostalcode);
                                }

                            }

                            $("#shippingaptno").val(corporateaptno);
                            $("#shippinghouseno").val(corporatehouseno)

                            $("#shippingstreet").val(corporatestreet);
                            $("#shippingcity").val(corporatecity);
                            $("#shippingstate").val(corporatestate);
                            $("#shippingpostalcode").val(corporatepostalcode);


                        }



                        $('#dialog').dialog('close');
                    }
                    else {
                        alert("Please fill empty fields.");
                    }
                }

            });

            $("#btnAddShipping").click(function () {


                var corporateaptno = $("#shippingaptno").val();
                var corporatehouseno = $("#shippinghouseno").val();
                var corporatestreet = $("#shippingstreet").val();
                var corporatecity = $("#shippingcity").val();
                var corporatestate = $("#shippingstate").val();
                var corporatepostalcode = $("#shippingpostalcode").val();

                $("#hdshiptno").val(corporateaptno);
                $("#hdshiphno").val(corporatehouseno);
                $("#hdshipstr").val(corporatestreet);
                $("#hdshipci").val(corporatecity);
                $("#hdshipst").val(corporatestate);
                $("#hdshippcode").val(corporatepostalcode);


                if (($("#shippingstreet").val() != "") && ($("#shippingcity").val() != "")
                    && ($("#shippingstate").val() != "") && ($("#shippingpostalcode").val() != "")) {



                    if (corporateaptno != "" && corporatehouseno != "") {
                        $("#shippingaddress").text(corporateaptno + '-' + corporatehouseno + '\r\n' + corporatestreet + '\r\n' + corporatecity + ' ' + corporatestate + '\r\n' + corporatepostalcode);
                    }
                    else {

                        if (corporateaptno == "" && corporatehouseno == "") {
                            $("#shippingaddress").text(corporatestreet + '\r\n' + corporatecity + ' ' + corporatestate + '\r\n' + corporatepostalcode);
                        }
                        else {
                            $("#shippingaddress").text(corporateaptno + ' ' + corporatehouseno + '\r\n' + corporatestreet + '\r\n' + corporatecity + ' ' + corporatestate + '\r\n' + corporatepostalcode);
                        }

                    }

                    $('#dialog1').dialog('close');
                }
                else {
                    alert("Please fill empty fields.");
                }


            });


        });

    </script>
